extends _layout

block vars
	- array = {body_class:'', layout_mod:'', sub_keywords:'INTERIOR DESIGN FOR WP', title_inner:'INTERIOR DESIGN'}
block content_block
	section.hero
		.hero_img 
			img(src="i/hero.jpg", alt="")
		.hero_main
			.hero_logotype 
				+heroIcon
	section.team.sec_anim 
		.section_in 
			.team_main
				+logo-grey 
				.team_content.el_anim
					h2 команда
					.global_txt 
						p Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.
						p Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.
				.team_people_w.el_anim 
					ul.team_people 
						-
							var people = [
								{
									title: 'татьяна мельникова',
									img: 'people_blonde'
								},
								{
									title: 'анастасия кацюба',
									img: 'people_dark'
								},
							]
						each item in people 
							li.people_item 
								.people_img 
									img(src=`i/${item.img}.jpg`, alt=item.title)
								.people_item_title 
									h3=item.title
	section.projecthead.sec_anim 
		.section_in 
			.projecthead_main
				+logo-grey
				.projecthead_title.el_anim 
					h3 наши проекты
