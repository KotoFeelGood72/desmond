	section.projecthead.serviceHead.sec_anim 
		.section_in 
			.projecthead_main
				+logo-grey
				.projecthead_title.el_anim 
					h3 услуги
	section.service.sec_anim
		+logo-grey 
		.section_in 
			.service_main 
				ul.service_list 
					each item in ['Разработка дизайн-проекта', 'Составление сметы', 'Реализация', 'Комплектация', 'Подбор предметов искусства', 'Декорирование']
						li.el_anim=item
				.service_img 
					img(src="i/doublegirl.jpg", alt="")
	section.public.sec_anim
		+logo-grey 
		.section_in 
			.public_main
				h3.el_anim публикации
				ul.public_list
					each item in ['ad', 'houzz', 'id']
						li.animation_el.el_anim 
							a(href="#")
								img(src=`i/${item}.svg`, alt="") 
	section.contacts.sec_anim
		+logo-grey 
		.section_in 
			.contacts_main 
				h3.el_anim Контакты
